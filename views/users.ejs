<!DOCTYPE html>
<html lang="es">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Listado de Usuarios</title>
    <link rel="stylesheet" href="/resources/styles.css">
</head>
<body>
    <h1>Listado de Usuarios</h1>
    <table border="1">
        <thead>
            <tr>
                <th>ID</th>
                <th>Usuario</th>
                <th>Nombre</th>
                <th>Correo</th>
                <th>Rol</th>
            </tr>
        </thead>
        <tbody>
            <% users.forEach(user => { %>
                <tr>
                    <td><%= user.user_id %></td>
                    <td><%= user.user %></td>
                    <td><%= user.name %></td>
                    <td><%= user.correo %></td>
                    <td><%= user.rol %></td>
                </tr>
            <% }); %>
        </tbody>
    </table>
</body>
</html>
